  "bugs": {
    "url": "https://github.com/Quinten/tiny-verf-game/issues"
  },
  "homepage": "https://github.com/Quinten/tiny-verf-game#readme",
  "dependencies": {
    "verf": "^0.2.3"
  },
  "devDependencies": {
    "html-webpack-plugin": "^4.3.0",
    "terser-webpack-plugin": "^3.0.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
